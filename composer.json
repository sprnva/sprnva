{
    "name": "sprnva/sprnva",
    "type": "project",
    "description": "The Sprnva Framework.",
    "keywords": [
        "framework",
        "sprnva"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Judywen Guapin",
            "email": "jguapin21@gmail.com"
        }
    ],
    "require": {
        "sprnva/fortify": "^1.0",
        "sprnva/framework": "^1.3"
    },
    "autoload": {
        "classmap": [
            "./"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('config.php') || copy('config.example', 'config.php');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
